A,GAAA;AACA,KAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,KARA,EASA,OATA,CASA;AAAA,aAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,KATA;AAWA,IAAA,KAAA,CACA,GADA,CACA,uDADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,KARA,EASA,OATA,CASA;AAAA,aAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,KATA;AAUA;AA9CA,CAAA","sourcesContent":["<template>\r\n    <div id=\"zap\">\r\n        <h1>Bitcoin Price</h1>\r\n\r\n        <section v-if=\"errored\">\r\n            <p>Price currently not available</p>\r\n        </section>\r\n\r\n        <section v-else>\r\n            <div v-if=\"loading\">Loading...</div>\r\n            <div v-else>\r\n                <button v-on:click=\"isShown = 1\">USD</button>\r\n                <button v-on:click=\"isShown = 2\">EUR</button>\r\n                <button v-on:click=\"isShown = 3\">GBP</button>\r\n                <button v-on:click=\"isShown = 4\">LTL</button>\r\n\r\n                <h1 v-if=\"isShown == 0\">...</h1>\r\n\r\n                <h1 v-if=\"isShown == 1\"> \r\n                    {{info.USD.description}}:  \r\n                    <span class=\"lighten\">\r\n                        <span v-html=\"info.USD.symbol\"></span>{{ info.USD.rate_float | currencydecimal }}\r\n                    </span>\r\n                </h1>\r\n\r\n                <h1 v-if=\"isShown == 2\"> \r\n                    {{info.EUR.description}}:  \r\n                    <span class=\"lighten\">\r\n                        <span v-html=\"info.EUR.symbol\"></span>{{ info.EUR.rate_float | currencydecimal }}\r\n                    </span>\r\n                </h1>\r\n\r\n                <h1 v-if=\"isShown == 3\"> \r\n                    {{info.GBP.description}}:  \r\n                    <span class=\"lighten\">\r\n                        <span v-html=\"info.GBP.symbol\"></span>{{ info.GBP.rate_float | currencydecimal }}\r\n                    </span>\r\n                </h1>\r\n\r\n                <h1 v-if=\"isShown == 4\"> \r\n                    {{ltlinfo.LTL.description}}: Lt {{ ltlinfo.LTL.rate_float | currencydecimal }}\r\n                </h1>\r\n\r\n\r\n            </div>\r\n        </section>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from \"axios\";\r\n    export default{\r\n        el: '#zap',\r\n        data () {\r\n            return {\r\n                info: null,\r\n                ltlinfo: null,\r\n                loading: true,\r\n                errored: false,\r\n                isShown: 0,\r\n                isHiddenEUR: false,\r\n                isHiddenGBP: false,\r\n                isHiddenLTL: false\r\n            }\r\n        },\r\n        filters: {\r\n            currencydecimal (value) {\r\n                return value.toFixed(2)\r\n            }\r\n        },\r\n        methods: {\r\n            say: function (message) {\r\n                alert(message)\r\n            }\r\n        },\r\n        mounted () {\r\n            axios\r\n                .get('https://api.coindesk.com/v1/bpi/currentprice.json')\r\n                .then(response => {\r\n                    this.info = response.data.bpi\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                    this.errored = true\r\n                })\r\n                .finally(() => this.loading = false)\r\n\r\n            axios\r\n                .get('https://api.coindesk.com/v1/bpi/currentprice/LTL.json')\r\n                .then(response => {\r\n                    this.ltlinfo = response.data.bpi\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                    this.errored = true\r\n                })\r\n                .finally(() => this.loading = false)\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n .button{\r\n     bottom: 100px;\r\n }\r\n</style>"],"sourceRoot":"src/components"}]}