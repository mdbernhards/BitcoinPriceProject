this.labels.xLabels.push(moment(key).format(\"MM/DD\"));\r\n            }\r\n        });\r\n    }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    #app {\r\n    margin: 0 auto;\r\n    padding: 30px;\r\n    max-width: 1200px;\r\n    max-height: 300px;\r\n    }\r\n\r\n    .vtc {\r\n        height: 250px;\r\n        font-size: 12px;\r\n        @media (min-width: 699px) {\r\n        height: 350px;\r\n        }\r\n    }\r\n    .grid,\r\n    .labels {\r\n        stroke: rgba(#f69119, 0.5);\r\n    }\r\n    \r\n        line {\r\n            opacity: 0.3;\r\n        }\r\n    .curve-btc {\r\n        stroke: #f69119;\r\n        stroke-width: 2;\r\n\r\n        fill: url(#btcFill);\r\n        fill-opacity: 0.5;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}